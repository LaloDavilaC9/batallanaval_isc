    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical">

        <RadioGroup
            android:id="@+id/grupoBarcos"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical">

            <RadioButton
                android:id="@+id/barco0"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Barco de 5 Casillas" />

            <RadioButton
                android:id="@+id/barco1"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Barco de 4 Casillas" />

            <RadioButton
                android:id="@+id/barco2"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Barco de 3 Casillas" />

            <RadioButton
                android:id="@+id/barco3"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Barco de 3 Casillas" />

            <RadioButton
                android:id="@+id/barco4"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Barco de 1 Casillas" />
        </RadioGroup>

        <Button
            android:id="@+id/botonConfirmar"
            android:layout_width="157dp"
            android:layout_height="wrap_content"
            android:layout_below="@+id/grupoBarcos"
            android:layout_alignParentStart="true"
            android:text="Confirmar" />

        <RadioGroup
            android:id="@+id/grupoDirecciones"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@+id/grupoBarcos"
            android:layout_toEndOf="@+id/botonConfirmar"
            android:orientation="horizontal">

            <RadioButton
                android:id="@+id/botonHorizontal"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Horizontal" />

            <RadioButton
                android:id="@+id/botonVertical"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Vertical" />
        </RadioGroup>

