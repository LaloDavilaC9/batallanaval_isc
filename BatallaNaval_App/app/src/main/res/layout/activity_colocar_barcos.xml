        android:layout_height="match_parent"
        android:orientation="vertical">

        <RadioGroup
            android:id="@+id/grupoBarcos"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical">

            <RadioButton
                android:id="@+id/barco0"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Barco de 5 Casillas" />

            <RadioButton
                android:id="@+id/barco1"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Barco de 4 Casillas" />

            <RadioButton
                android:id="@+id/barco2"
